# External hostnames
N8N_HOST=n8n.dev.company
WEBHOOK_URL=https://hooks.dev.company

# Behind one proxy (Nginx)
N8N_PROXY_HOPS=1

# n8n server internals
N8N_PROTOCOL=https
N8N_PORT=5678
NODE_ENV=production
GENERIC_TIMEZONE=Asia/Manila
TZ=Asia/Manila

# Trust your private CA for outbound HTTPS calls from n8n (Node.js)
NODE_EXTRA_CA_CERTS=/etc/ssl/private-ca/ca-root.pem
